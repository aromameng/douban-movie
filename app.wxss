/**app.wxss**/

/* 公共样式  */
.c-inline{
  display: inline-block;
}
.c-gray{
  color: #999;
}

.container {
  height: 100%;
  box-sizing: border-box;
} 
.page-session{
  width: 100%;
  margin-bottom: 20px;
}
.session-content {
  padding: 0rpx 30rpx 0rpx 30rpx;
}

.session-header {
  padding: 0rpx 30rpx 0rpx 30rpx;
}

.session-headeer .session-title {
  color: #4a4a4a;
  font-size: 32rpx;
  font-weight: 500;
  flex: 1;
}

.session-header .more {
  color: #32cd32;
  font-size: 28rpx;
  margin-right: 20rpx;
  position: relative;
  float: right;
}

.session-header .more::after {
  content: " ";
  display: inline-block;
  position: absolute;
  width: 12rpx;
  height: 12rpx;
  border-width: 1px 1px 0px 0px;
  border-style: solid;
  border-color: #32cd32;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  top: 50%;
  margin-top: -8rpx;
}

.event-scroll {
  width: 100%;
  height: 400rpx;
  white-space: nowrap;
}
.session-content .items{
  padding: 20rpx 0;
}
.session-content .items .item{
  display: inline-block;
  text-align: center;
  margin-left: 10px;
    width:200rpx;
    overflow: hidden;
}
.session-content .items .item:first-child{
  margin-left: 0;
}
.session-content .items .item-image{
  display: block;
  width:200rpx;
  height: 285rpx;
}
.session-content .items .item-title{
  width: 100%;
  display: block;
   line-height:30px;
   height: 30px;
   font-size: 28rpx;
   overflow: hidden;
   text-overflow: ellipsis;
   word-break: break-all;
}
